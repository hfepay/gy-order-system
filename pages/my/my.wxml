<!--pages/my/my.wxml-->
<view class="container">
  <view class="user-info">
    <view class="avatar">
      <open-data  type="userAvatarUrl"></open-data>
    </view>
    <view class="detail">
      <view class="username">
        <open-data  type="userNickName"></open-data>
      </view>
      <view class="wechat">微信号:XXXXXXXXXX</view>
    </view>
  </view>
  <view class="cell-container">
    <van-cell title="我的资料" is-link url="/pages/my_info/my_info"/>
    <van-cell title="收货地址" is-link url="/pages/address/address"/>
    <van-cell title="问题反馈" is-link url="/pages/feedback/feedback"/>
  </view>
  <view class="logout">
    <navigator url="/pages/login/login">
      <van-button block>退出</van-button>
    </navigator>
  </view>
</view>
